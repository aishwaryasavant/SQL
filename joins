	;
    /*cross join is cartesian product of the tables pairing one row of the table with all rows from another table
    m*n time data where m is rows from one table and n is rows from another table*/ 
    select * from j1 cross join j2;
    
    /*its a combination of left join and right join 
    returns matching recordds from left table
    and returns matchin records from right table*/
    
        select * from j1 full outer join j2
        on j1.id = j2.id;

